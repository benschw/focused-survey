<fieldset>
	<div class="content">
		<h2>New Account</h2>

		<label for="loginEmail">Full Email</label>
		<input class="float" type="text" id="loginEmail" name="email" />
		<div class="clear"></div>

		<label for="password">Password</label>
		<input class="float" type="password" id="password" name="password" />
		<div class="clear"></div>

		<label for="password">Confirm<br />Password</label>
		<input class="float" type="password" id="password2" name="passwordConfirm" />
